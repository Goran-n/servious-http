{
  "name": "servious-http",
  "version": "1.0.0",
  "private": true,
  "description": "Servious for HTTP",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --exec babel-node src/index.js",
    "build": "babel src -D -d dist"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
